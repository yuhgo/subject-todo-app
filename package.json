{
  "name": "todo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 4000",
    "lint": "run-p --continue-on-error lint:*",
    "lint:eslint": "eslint --ext .jsx,.js,.tsx,.tsx .",
    "lint:prettier": "prettier --write .",
    "fix": "run-s --continue-on-error fix:eslint fix:prettier",
    "fix:eslint": "eslint --ext .jsx,.js,.tsx,.tsx . --fix",
    "fix:prettier": "prettier --write .",
    "prepare": "husky install",
    "unit": "jest --watch",
    "unit:build": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/server": "^11.4.0",
    "@mantine/core": "^5.0.0",
    "@mantine/ds": "^5.0.0",
    "@mantine/form": "^5.0.0",
    "@mantine/hooks": "^5.0.0",
    "@mantine/modals": "^5.0.0",
    "@mantine/next": "^5.0.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@tabler/icons": "^1.78.1",
    "classcat": "^5.0.3",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-redux": "^8.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/node": "18.6.2",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.12",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-access": "1.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "next-page-tester": "^0.33.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.6",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn fix:eslint",
      "yarn fix:prettier"
    ],
    "**/*.{json,md,mdx,css,html,yml,yaml,scss}": [
      "prettier --write ."
    ]
  }
}
